{
    "engine" : "spacy",
    "description" : "Spacy_Regular_Expression",
    "version" : "1.1",
    "type" : "REGEXP",
    "requirements" : [
        "spacy",
        "others"
    ],

    "lists" : [
        {
            "name" : "Email",
            "data" : "^.*@.*..*$",
            "label" : "EMAIL",
            "weight" : 1
        },
        {
            "name" : "Codice_Fiscale",
            "data" :"^[A-Z]{6}[0-9]{2}[A-Z][0-9]{2}[A-Z][0-9]{3}[A-Z]$",
            "label" : "FISCAL_CODE",
            "weight" : 1
        },
        {
            "name" : "Targa_Auto",
            "data" : "^[A-Z]{2}[0-9]{3}[A-Z]{2}$",
            "label" : "TARGA_AUTO",
            "weight" : 1
        }
        
    ]
}